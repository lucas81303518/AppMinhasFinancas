unit Controller.Gastos;

interface

uses
  DAO.Gastos;

type
  TControllerGastos = class
  private
    FDAOGastos: TDAOGastos;
  public
    constructor Create(); override;
    destructor Destroy;
end;

implementation

{ TControllerGastos }

constructor TControllerGastos.Create();
begin
  inherited;
  FDAOGastos := TDAOGastos.Create;
end;

destructor TControllerGastos.Destroy;
begin
  FDAOGastos.Destroy;
  inherited;
end;

end.
